{
  "$schema": "https://json.schemastore.org/staticwebapp.config.json",
  "globalHeaders": {
    "X-Robots-Tag": "noindex, nofollow, noarchive"
  },
  "navigationFallback": {
    "rewrite": "/404.html",
    "exclude": [
      "/assets/*",
      "/favicon.png",
      "/robots.txt",
      "/security.txt",
      "/api/*",
      "/s/preview-8b3c9e/*"
    ]
  },
  "routes": [
    {
      "route": "/",
      "statusCode": 404
    },
    {
      "route": "/index.html",
      "statusCode": 404
    },
    {
      "route": "/s/preview-8b3c9e/*",
      "rewrite": "/index.html"
    },
    { "route": "/assets/*" },
    { "route": "/favicon.png" },
    { "route": "/robots.txt" },
    { "route": "/security.txt" },
    { "route": "/api/*" },
    {
      "route": "/*",
      "statusCode": 404
    }
  ]
}
